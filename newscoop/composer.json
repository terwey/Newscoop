{
    "name":"sourcefabric/newscoop",
    "description":"The open source CMS for news",
    "keywords":[
        "cms",
        "journalism",
        "publication"
    ],
    "homepage":"http://www.sourcefabric.org/en/newscoop/",
    "version":"4.1",
    "license":"GPL-3.0",
    "authors":[
        
    ],
    "require":{
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "bombayworks/zendframework1":"1.11.*",
        "friendsofsymfony/rest-bundle": "0.13.*@dev",
        "smarty/smarty": "3.0.9",
        "behat/behat":">=2.4.0",
        "behat/common-contexts":"dev-master#b9f0dc44d415e94a03152ca6413c89e21889ab56",
        "kriswallsmith/buzz":">=0.7",
        "knplabs/knp-paginator-bundle":"v2.3.2",
        "jms/serializer-bundle":"0.12.x-dev#ab2ca25",
        "guzzle/guzzle":"dev-master#f2fd4bd43bbdf0bda02f0db6458b7e93fe2d8236",
        "rezzza/mailchimp":"dev-master",
        "hybridauth/hybridauth":"dev-master",
        "noiselabs/smarty-bundle":"dev-master#50de401fd7ddfdc78b7d65fc113e073783f38233",
        "knplabs/knp-menu-bundle": "2.0.*@dev",
        "knplabs/knp-menu": "2.0.*@dev",
        "raulfraile/ladybug-bundle": "dev-ladybug_v1.0",
        "friendsofsymfony/jsrouting-bundle": "~1.1",
        "willdurand/expose-translation-bundle": "0.3.*@dev",
        "recaptcha/php5": "v1.0.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "smarty/smarty",
                "version": "3.0.9",
                "dist": {
                    "url": "http://www.smarty.net/files/Smarty-3.0.9.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["libs/"]
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/terwey/SmartyBundle"
        }
    ],
    "require-dev":{
        "phpunit/phpunit":"3.7.*"
    },
    "autoload":{
        "psr-0":{
            "Newscoop":[
                "src",
                "library"
            ],
            "Resource":"library",
            "Nette":"library",
            "Proxy":"library"
        },
        "classmap":[
            "classes",
            "template_engine",
            "plugins",
            "include",
            "application"
        ]
    },
    "include-path":[
        "include",
        "./../dependencies/include"
    ],
    "scripts":{
        "post-install-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Newscoop\\Composer\\ScriptHandler::generateOrmProxies"
        ],
        "post-update-cmd":[
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Newscoop\\Composer\\ScriptHandler::generateOrmProxies"
        ]
    },
    "minimum-stability":"dev",
    "config":{
        "bin-dir":"vendor/bin"
    },
    "extra":{
        "symfony-app-dir":"application",
        "symfony-web-dir":"public"
    },
    "branch-alias":{
        "dev-master":"2.3-dev"
    }
}
